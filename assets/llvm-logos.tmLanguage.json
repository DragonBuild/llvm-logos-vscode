{
	"fileTypes": [
		"m",
		"mm"
	],
	"name": "Objective-C",
	"patterns": [
		{
			"match": "@(init|hookf|hook|subclass|group|class|new|ctor|dtor|end|config|orig|log|property|c)",
			"name": "keyword.source.llvm-logos"
		},
		{
			"match": "@hook *"
		},
		{
			"captures": {
				"1": {
					"name": "keyword.source.llvm-logos"
				}
			},
			"match": "@c([A-Za-z$_]+)"
		},
		{
			"include": "source.objc"
		},
		{
			"include": "source.c++"
		}
	],
	"scopeName": "source.llvm-logos"
}
